package calculator;

public class Functions {
	public static int factorial(double firstDoubleValue) {
		double Answer = 1;
		if(firstDoubleValue == 0 || firstDoubleValue == 1) {
			Answer = 1;
		} else {
			Answer = 1;
			for (int i = 2; i <= firstDoubleValue; i++) {
				Answer *= i; 
			}
		}
		return (int) Answer;
	}
}